#ifndef BUZZER_H_
#define BUZZER_H_
#include"std_types.h"
#define BUZZER_PORT_ID  PORTD_ID
#define BUZZER_PIN_ID   PIN5_ID

void buzzer_init(void)
{
	GPIO_setupPinDirection(BUZZER_PORT_ID, BUZZER_PIN_ID, PIN_OUTPUT);
}

#endif /* BUZZER_H_ */
